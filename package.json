    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "exp-amqp-connection": "BonnierNews/exp-amqp-connection#v2.9.0",
    "exp-config": "^2.0.1",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "moment": "^2.24.0",
    "popper.js": "^1.14.7",
    "react": "^16.5.2",
    "react-bootstrap-table-next": "^2.1.0",
